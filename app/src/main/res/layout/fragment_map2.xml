<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.yang_deng.monashfriendfindertest.MapActivityForFriends"
    tools:layout_editor_absoluteY="25dp"
    tools:layout_editor_absoluteX="0dp">

    <Spinner
        android:id="@+id/spinner_distance"
        android:layout_width="181dp"
        android:layout_height="51dp"
        tools:layout_editor_absoluteY="0dp"
        android:layout_marginLeft="16dp"
        android:entries="@array/Distance_array"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/button_distance"
        android:layout_width="137dp"
        android:layout_height="52dp"
        android:layout_marginTop="0dp"
        android:text="Check"
        app:layout_constraintHorizontal_bias="0.95"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="258dp" />

    <com.mapquest.mapping.maps.MapView
        android:id="@+id/mapquestMapView2"
        android:layout_width="400dp"
        android:layout_height="650dp"
        app:mapquest_app_key="ZwgKX7yAPOgbAspuvIZ3GMTEBfZdjB0e"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/spinner_distance" />

</android.support.constraint.ConstraintLayout>
